/* pages/index/index.wxss */
swiper{
  height: 421.5rpx;
}
.slide-image{
  width: 100%;
  height: 100%;
}
.selected{
  position: relative;
}
.select-title{
  width: 100%;
  padding: 30rpx 0;
  height: 40rpx;
  color: #ab956a;
  text-align: center;
}
.select-top,.select-bottom{
  width: 100%;
  height: 357rpx;
}
.select-top-small{
  width: 375rpx;
  height: 375rpx;
  float: left;
}
.select-top-small image,
.select-top-small navigator,
.select-bottom navigator,
.select-bottom image{
  display: block;
  width: 100%;
  height: 100%;
}


.newest-title{
  width: 100%;
  padding: 30rpx 0;
  height: 40rpx;
  color: #ab956a;
  text-align: center;
}
.newest-box{
  padding:0 40rpx;
}
.newest-box .newest-list{
  display: inline-block;
  width: 325rpx;
  height: 325rpx;
  margin:0 20rpx 20rpx 0;
  border-radius: 10px;
  text-align: center;
  background: #f5f6f5;
}
.newest-box .newest-list:nth-child(2n){
  margin-right: 0;
}
.newest-box .newest-list image{
  width: 175rpx;
  height: 175rpx;
  margin: 20rpx 0 10rpx;
}
.newest-box .newest-list .newest-text{
  font-size: 32rpx;
}
